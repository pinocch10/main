  <log4net debug="false">
    <appender name="DefaultCompleteAppender" type="log4net.Appender.RollingFileAppender">
      <file value="log\internal\" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <staticLogFileName value="false" />
      <datePattern value="yyyy-MM-dd'_flow_complete.log'" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date|%level|%message%newline" />
      </layout>
    </appender>
    <appender name="ExceptionAppender" type="log4net.Appender.RollingFileAppender">
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="FATAL" />
        <levelMax value="FATAL" />
      </filter>
      <file value="log\internal\" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <staticLogFileName value="false" />
      <datePattern value="yyyy-MM-dd'_flow_exception.log'" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date|%-5level|%message%newline" />
      </layout>
    </appender>
    <appender name="ErrorAppenderAfisha" type="log4net.Appender.RollingFileAppender">
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="Error" />
        <levelMax value="FATAL" />
      </filter>
      <file value="log\internal\" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <staticLogFileName value="false" />
      <datePattern value="yyyy-MM-dd'_flow_error_afisha.log'" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date|%-5level|%message%newline" />
      </layout>
    </appender>
    <appender name="MissingCinemaAppender" type="log4net.Appender.RollingFileAppender">
      <file value="log\internal\" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <staticLogFileName value="false" />
      <datePattern value="yyyy-MM-dd'_missing_cinemas.log'" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date|%level|%message%newline" />
      </layout>
    </appender>
    <appender name="CommunicationAppender" type="log4net.Appender.RollingFileAppender">
      <file value="log\internal\" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <staticLogFileName value="false" />
      <datePattern value="yyyy-MM-dd'_communication.log'" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date|%level|%message%newline" />
      </layout>
    </appender>

    <appender name="KinopoiskDataAppender" type="log4net.Appender.RollingFileAppender">
      <file value="log\internal\" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <staticLogFileName value="false" />
      <datePattern value="yyyy-MM-dd'_kinopoisk_data.log'" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date|%level|%message%newline" />
      </layout>
    </appender>
    <logger name="MissingCinemaLogger" additivity="false">
      <level value="ALL" />
      <appender-ref ref="MissingCinemaAppender" />
    </logger>

    <logger name="KinopoiskDataLogger" additivity="false">
      <level value="ALL" />
      <appender-ref ref="KinopoiskDataAppender" />
    </logger>
    <logger name="ErrorLoggerAfisha">
      <level value="ALL" />
      <appender-ref ref="ErrorAppenderAfisha" />
    </logger>
    <logger name="CommunicationLogger" additivity="false">
      <level value="ALL" />
      <appender-ref ref="CommunicationAppender" />
    </logger>
    <logger name="DefaultLogger">
      <level value="ALL" />
      <appender-ref ref="DefaultCompleteAppender" />
    </logger>
    <root>
      <priority value="ALL" />
      <appender-ref ref="DefaultCompleteAppender" />
      <appender-ref ref="ExceptionAppender" />
    </root>
  </log4net>
